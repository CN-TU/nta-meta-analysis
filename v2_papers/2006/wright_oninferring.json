        "role": "main",
        "main_goal": "application_classification",
        "active_timeout": "missing",
        "idle_timeout": "missing",
        "bidirectional": true,
        "features": [
          {
            "apply": [
              {
                "multiply": [
                  "octetTotalCount",
                  {
                    "subtract": [
