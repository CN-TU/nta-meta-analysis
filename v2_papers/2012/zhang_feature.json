    "flows": [
      {
        "selection": "feature_selection",
        "role": "main",
        "main_goal": "application_classification",
        "bidirectional": true,
        "features": [
          "destinationTransportPort",
          {
            "minimum": [
              "octetTotalCount"
            ]
          },
          {
            "apply": [
              "__initialWindowBytes",
              "forward"
            ]
          },
          {
            "apply": [
              "__initialWindowBytes",
              "backward"
            ]
          }
        ],
        "key_features": [
          "protocolIdentifier",
          "sourceIPv4Address",
          "sourceTransportPort",
          "destinationIPv4Address",
          "destinationTransportPort"
        ],
