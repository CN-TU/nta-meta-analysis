        "selection": "feature_selection",
        "role": "competitor",
        "main_goal": "application_classification",
        "bidirectional": true,
        "features": [
          "destinationTransportPort",
          "tcpPshTotalCount",
          {
            "get": [
              "_maximumSegmentSize",
