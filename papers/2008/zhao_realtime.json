{
  "flows": [
    {
      "goals": [
        "trafficClassification"
      ], 
      "key": {
        "bidirectional": "separate_directions", 
        "key_features": [
          "sourceIPv4Address", 
          "sourceTransportPort", 
          "destinationIPv4Address", 
          "destinationTransportPort", 
          "protocolIdentifier"
        ]
      }, 
      "tool": null,
      "window": null,
      "traffic_type": "tcp",
      "features": [
        {"sum": [{"slice": [{"map": ["_interPacketTimeMicroseconds", {"select": [true]}]}, 0, {"divide": [{"count": [{"select": [true]}]}, 4]}]}]},
        {"sum": [{"slice": [{"map": ["ethernetTotalLength", {"select": [true]}]}, 0, {"divide": [{"count": [{"select": [true]}]}, 4]}]}]},
        {"sum": [{"slice": [{"map": ["ipTotalLength", {"select": [true]}]}, 0, {"divide": [{"count": [{"select": [true]}]}, 4]}]}]},
