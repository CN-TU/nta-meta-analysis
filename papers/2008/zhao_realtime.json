        {"sum": [{"slice": [{"map": [{"basedon": ["_interPacketTimeMicroseconds", "_server_to_client"]}, {"select": [true]}]}, 0, {"divide": [{"count": [{"select": [true]}]}, 4]}]}]},
        {"sum": [{"slice": [{"map": [{"basedon": ["ethernetTotalLength", "_server_to_client"]}, {"select": [true]}]}, 0, {"divide": [{"count": [{"select": [true]}]}, 4]}]}]},
        {"sum": [{"slice": [{"map": [{"basedon": ["ipTotalLength", "_server_to_client"]}, {"select": [true]}]}, 0, {"divide": [{"count": [{"select": [true]}]}, 4]}]}]},
        {"sum": [{"sum": [{"slice": [{"map": [{"basedon": ["ethernetHeaderLength", "_server_to_client"]}, {"select": [true]}]}, 0, {"divide": [{"count": [{"select": [true]}]}, 4]}]}]}, {"sum": [{"slice": [{"map": [{"basedon": ["ipHeaderLength", {"select": [true]}]}, 0, {"divide": [{"count": [{"select": [true]}]}, 4]}]}]}, {"sum": [{"slice": [{"map": [{"basedon": ["tcpHeaderLength", {"select": [true]}]}, 0, {"divide": [{"count": [{"select": [true]}]}, 4]}]}]}]}]}]}
